class Solution {
    public int longestSubsequence(int[] nums) {
        int[] a = nums;
        
        int totalXor = 0;
        for (int num : a) {
            totalXor ^= num;
        }
        
        if (totalXor != 0) {
            return a.length;
        } else {
            for (int num : a) {
                if (num != 0) return a.length - 1;
            }
            return 0;
        }
    }
}
