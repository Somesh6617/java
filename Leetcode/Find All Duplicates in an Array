class Solution {
    public List<Integer> findDuplicates(int[] nums) {
    List<Integer> a=new ArrayList<>();
    //Arrays.sort(nums);
    int a1[]=new int[nums.length];
    for(int i=0;i<nums.length;i++)
        a1[nums[i]-1]++;
    for(int i=0;i<nums.length;i++)
    {
        if(a1[i]==2)
            a.add(i+1);
    }
    return a;
    }
}
