import java.util.*;
import java.io.*;

class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt(),d=sc.nextInt();
        long c=0;
        for(int i=0;i<n;i++)
        {
            long x=sc.nextLong();
            x/=10;
            if(x%10==d)
                c++;
        }
        System.out.println(c);
    }
}
