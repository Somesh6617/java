class Solution {
    public String convertDateToBinary(String date) {
    String s[]=date.split("-");
    String s1="";
    for(int i=0;i<s.length;i++)
    {
        int a=Integer.parseInt(s[i]);
        String s2=Integer.toBinaryString(a);
        s1+=s2;
        if(i<s.length-1)
            s1+="-";
    }
    return s1;
    }
}
