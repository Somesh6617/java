import java.util.*;
import java.io.*;

class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        while(t-->0)
        {
            char ch=sc.next().charAt(0);
            if(Character.isLowerCase(ch))
                ch=Character.toUpperCase(ch);
            else
                ch=Character.toLowerCase(ch);
            System.out.println(ch);
        }
    }
}
