class Solution {
    public boolean isPalindrome(int x) {
        int n=x,rem=0,rev=0;
        if(x<0)
            return false;
        else
        {
        while(x>0)
        {
            rem=x%10;
            rev=rev*10+rem;
            x/=10;
        }
            return n==rev;
        }
    }
}
