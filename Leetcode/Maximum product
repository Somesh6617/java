class Solution {
    public int maxProduct(int[] nums) {
        int l=Integer.MIN_VALUE,sl=Integer.MIN_VALUE;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]>l)
            {
                sl=l;
                l=nums[i];
            }
            else if(nums[i]>sl) sl=nums[i];
        }
        return (l-1)*(sl-1);
    }
}
