class Solution {
    public int missingMultiple(int[] nums, int k) {
    Set<Integer> a=new HashSet<>();
    for(int i=0;i<nums.length;i++)
    {
        if(nums[i]%k==0)
        {
          int b=nums[i];  
          a.add(b);
        }
    }
    int b=0;
    for(int i=1;i<=a.size()+1;i++)
    {
        if(a.contains(k*i))
            continue;
        else
        {
            b=i*k;
            break;
        }
    }
        return b;
    }
}
