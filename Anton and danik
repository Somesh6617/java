import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        String s=sc.next();
        int d=0,a=0;
        for(int i=0;i<n;i++)
        {
            if(s.charAt(i)=='A')
                a++;
            else
                d++;
        }
        System.out.println(((a>d)?"Anton":(a<d)?"Danik":"Friendship"));
    }
}
