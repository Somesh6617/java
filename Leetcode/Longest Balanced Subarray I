class Solution {
    public int longestBalanced(int[] nums) {
        int res=0;
    for(int i=0;i<nums.length;i++)
    {
        Set<Integer> a=new HashSet<>();
        Set<Integer> b=new HashSet<>();
        for(int j=i;j<nums.length;j++)
        {
            int n=nums[j];
            if(n%2==0)
                a.add(n);
            else
                b.add(n);
            if(a.size()==b.size())
                res=Math.max(res,j-i+1);
        }
    }
        return res;
    }
}
