  public static String removeOuterParentheses(String s) {
      StringBuilder sb=new StringBuilder();
      int n=0;
      for(char ch:s.toCharArray())
      {
          if(ch=='(')
          {
              if(n>0)
              {
                  sb.append(ch);
              }
              n++;
          }
          else
          {
              n--;
              if(n>0)
              {
                  sb.append(ch);
              }
          }
      }
      return sb.toString();
  }
