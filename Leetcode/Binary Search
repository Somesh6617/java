class Solution {
    public int search(int[] nums, int target) {
        return biPara(nums,target,0,nums.length-1);
    }
    public int biPara(int[] nums,int target,int l,int r)
    {
        if(l>r)
            return -1;
        int m=(l+r)/2;
        if(nums[m]==(target))
            return m;
        else if(nums[m]>target)
            return biPara(nums,target,l,m-1);
        else
            return biPara(nums,target,m+1,r);
    }
}
