 public static String findLargestOddSubstring(String num) {
     StringBuilder s=new StringBuilder();
     int x=-1;
     for(int i=num.length()-1;i>=0;i--)
     {
         if((num.charAt(i)-'0')%2!=0) 
         {
             x=i;
             break;
         }
     }
     for(int i=0;i<=x;i++)
         s.append(num.charAt(i));
     return x==-1? "-1":s.toString();
 }
