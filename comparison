import java.util.*;
import java.io.*;

class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        sc.nextLine();
        while(t-->0)
        {
            String s=sc.next();
            String r="";
            for(int i=s.length()-1;i>=0;i--)
                r+=s.charAt(i);
            System.out.println(r);
        }
    }
}
