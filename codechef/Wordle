import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		while(t-->0)
		{
		    String s=sc.next(),T=sc.next();
		    String m="";
		    for(int i=0;i<5;i++)
		    {
		        if(s.charAt(i)==T.charAt(i)) m+="G";
		        else m+="B";
		    }
		    System.out.println(m);
		}
	}
}
