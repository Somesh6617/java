class Solution {
    public boolean isValid(String s) {
        Stack<Character> st1=new Stack<>();
        for(int i=0;i<s.length();i++)
        {
            char ch=s.charAt(i);
            if(ch=='('||ch=='['||ch=='{')
            {
                st1.push(ch);
            }
            else
            {
                if (st1.isEmpty()) return false;

                char top = st1.pop();
                if ((ch == ')' && top != '(') ||
                    (ch == ']' && top != '[') ||
                    (ch == '}' && top != '{'))
               {
                    return false;
                }
            }
        }
         return st1.isEmpty();
    }
}
