import java.util.*;
import java.io.*;
 
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s=sc.next();
        int c=0;
        for(int i=0;i<s.length();i++)
        {
            if(Character.isLowerCase(s.charAt(i)))
                c++;
        }
        int n=s.length()-c;
        if(n>c)
            s=s.toUpperCase();
        else
            s=s.toLowerCase();
        System.out.println(s);
    }
}
