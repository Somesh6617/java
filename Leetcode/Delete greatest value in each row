class Solution {
    public int deleteGreatestValue(int[][] grid) {
        int ans=0,r=grid.length,c=grid[0].length;
        for(int i=0;i<r;i++) 
            Arrays.sort(grid[i]);
        for(int i=c-1;i>=0;i--)
        {
            int mx=0;
            for(int j=0;j<r;j++)
            {
                mx=Math.max(mx,grid[j][i]);
            }
            ans+=mx;
        }
        return ans;
    }
}
