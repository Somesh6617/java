import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner sc=new Scanner(System.in);
	    int t=sc.nextInt();
	    int e=0,o=0;
	    for(int i=0;i<t;i++)
	    {
	        int x=sc.nextInt();
	        if(x%2==0)
	            e++;
	        else
	            o++;
	    }
	    if(e>o) System.out.println("READY FOR BATTLE");
	    else System.out.println("NOT READY");

	}
}
