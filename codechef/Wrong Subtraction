import java.util.*;
import java.io.*;
 
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int s=sc.nextInt(),n=sc.nextInt();
        while(s>0 && n>0 )
        {
            int rem=s%10;
            if(rem>0)
                s-=1;
            else
                s/=10;
            n--;
        }
        System.out.println(s);
    }
}
